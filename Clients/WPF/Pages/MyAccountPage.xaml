<pages:MvWpfPage x:Class="NotatnikMechanika.WPF.Pages.MyAccountPage"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
            xmlns:d="http://schemas.microsoft.com/expression/blend/2008"       
            xmlns:core="clr-namespace:NotatnikMechanika.Core.PageModels;assembly=NotatnikMechanika.Core"
            xmlns:pages="clr-namespace:MvvmPackage.Wpf.Pages;assembly=MvvmPackage.Wpf"
            x:TypeArguments="core:MyAccountPageModel"
            xmlns:local="clr-namespace:NotatnikMechanika.WPF.Pages"
            mc:Ignorable="d" 
            d:DesignHeight="450" 
            Background="{DynamicResource MaterialDesignPaper}"
            FontFamily="{DynamicResource MaterialDesignFont}"
            d:DesignWidth="800">
    <Grid>
        <Button Content="Wyloguj" Command="{Binding LogoutCommand}"/>
    </Grid>
</pages:MvWpfPage>
